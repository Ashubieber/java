class Assig8
{
	static double triangle() 
	{
		int b = 5;
		int h = 8;
		double area = 0.5 * b * h;
		return area;
	}
	static int square()
	{
		int a = 5;
		int b=9;
		int length = a*b;
		return length;
	}
	static int rectangle()
	{
		int w = 9;
		int h = 7;
		int area = w * h;
		return area;
	}
	static int parall()
	{
		int b = 4;
		int h = 6;
		int area =b*h;
		return area;	
	}
	static double trapezoid()
	{
		int a = 5;
		int b = 9;
		int h = 7;
		double area = 0.5 *(a+b) *h;
		return area();
	}
	static double ellipse()
	{
		int a = 9;
		int b = 4;
		 final double pi = 3.142;
		 double area = pi * a * b;
		 return area;
	}
	static double sector()
	{
		int r = 4;
		double t = 35.4;
		double area = 0.5*r*r*t;
		return area;
	}
	public static void main(String[] args)
	{
		double x = triangle();
		System.out.println(x);
	
	
		int a = square();
		System.out.println(a);
	
	
		int b = rectangle();
		System.out.println(b);
	
	
		int c = parall();
		System.out.println(c);
	
	
		double d= trapezoid();
		System.out.println(d);
		double e = ellipse();
		System.out.println(e);
		double f = sector();
		System.out.println(f);
	
}}
